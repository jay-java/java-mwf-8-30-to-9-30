package core;

import java.util.Scanner;

public class ConditionalStatements {
	public static void main(String[] args) {
		int i,j,k;
		System.out.println("enter i = ");
		Scanner scanner = new Scanner(System.in);
		i = scanner.nextInt();
		System.out.println("entet j =");
		j = scanner.nextInt();
		
	}
}
