package core;

public class CommandLIne {
	public static void main(String[] args) {
		float pi = 3.14f;
		float a = 22f;
		float b = 7f;
		float c = a/b;
		System.out.println(c);
		float pi2 = 22/7;
		float r= 7f;
		double d = Math.PI;
		System.out.println(d);
		System.out.println(2*c*r);
		for(String s:args) {
			System.out.println(s);
		}
	}
}
